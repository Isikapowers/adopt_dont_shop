
   <%= form_with url: path, method: method, local: true do |f| %>
     <p>
       <%= f.label :name %>
       <%= f.text_field :name %>
     </p>
     <p>
       <%= f.label :address %>
       <%= f.text_field :address %>
     </p>
     <p>
       <%= f.label :city %>
       <%= f.text_field :city %>
     </p>
     <p>
       <%= f.label :state %>
       <%= f.text_field :state%>
     </p>
     <p>
       <%= f.label :zipcode %>
       <%= f.text_field :zipcode %>
     </p>
     <p>
       <%= f.label :foster_program %>
       <%= f.check_box :foster_program %>
     </p>
     <p>
       <%= f.label :rank %>
       <%= f.number_field :rank %>
     </p>
     <%= f.submit %>
   <% end %>
