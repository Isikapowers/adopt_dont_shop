<h1><%= @pet.name %></h1>
<h3><%= @pet.age %> years old</h3>
<h3><%= @pet.breed %></h3>
<h3><%= @pet.adoptable %></h3>
<h3><%= @pet.shelter_name %></h3>

<%= button_to "Update", "/pets/#{@pet.id}/edit", class: "btn btn-secondary" %>
<%= button_to "Delete", "/pets/#{@pet.id}", method: :delete, class: "btn btn-danger" %>
